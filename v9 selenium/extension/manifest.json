{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs",
      "storage",
      "activeTab",
      "scripting",
      "nativeMessaging"
    ],
    "externally_connectable": {
      "matches": ["http://localhost:5000/*"]
    },
    "action": {
      "default_icon": "icon.jpg"
    }
  }
  